@inproceedings{Stein2007,
 author = {Klauss Stein and Claudia Hess},
 title = {Does it matter who contributes: a study on featured articles in the german wikipedia},
 booktitle = {Proceedings of the 18th conference on Hypertext and hypermedia},
 series = {HT '07},
 year = {2007},
 isbn = {978-1-59593-820-6},
 pages = {171--174},
 location = {Manchester, UK},
 doi = {http://doi.acm.org/10.1145/1286240.1286290},
 publisher = {ACM},
 address = {New York, NY, USA},
 annote = {They define two metrics for author reputation:
 	the percentage of \textit{excellent} articles edited (page-based), and the percentage of
	edits that were made to \textit{excellent} articles (edit-based).
	The formulae are reversed.
	They show that there is a relationship between authors with high reputation
	and who edits \textit{worth reading} articles; in particular, if a high reputation
	author edits within the first 14 revisions, it is more likely that the
	article will become \textit{worth reading}.
    },
 }

