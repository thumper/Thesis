@article{Kittur2007,
    author = {Aniket Kittur and Ed Chi and Bryan A. Pendleton and Bongwon Suh and Todd Mytkowicz},
    title = {Power of the {F}ew vs. {W}isdom of the {C}rowd: {W}ikipedia and the rise of the {B}ourgeoisie},
    journal = {Alt.CHI},
    year = 2007,
    annote = {Paper 3, creates categories of users based on two metrics ---
        number of edits and amount of edits as measured by "diff".
    	They define 5 categories of users in terms of number of edits:
	(>10000), (5000, 10000], (1000, 5000], (100, 1000] and (<100).
	They argue that the decline in contributions by the Wikipedia
	elite (>10000 edits) is due to a rise in the contributions of the bourgeoisie (<100 edit users).
	One surprising thing they mention is that bots don't account for the
	reduction in the number of edits being done by the elite users.
	From our analysis, AntiVandalBot ranks highest in many metrics.
	
	Also, in 3, the authors state that measured in terms of number
	of edits or in terms of edit differences, they get similar results.
	All pointing to their title ``A reduction in elite participation
	accompanied by an increase in participation of the bourgeoisie''.
	},
}

@article{Korfiatis2006,
    author = {Nikolaos Th. Korfiatis and Marios Poulos and George Bokos},
    title = {Evaluating Authoritative Sources using Social Networks:
	an insight from {W}ikipedia},
    journal = {Online Information Review},
    volume = 30,
    number = 3,
    pages = {252--262},
    year = 2006,
    annote = {
        Wikipedia subject to criticism about trustworthiness
	(Fasoldt 2004 "Librarian: don't use Wikipedia as a source", others).
	Paper creates an authoritative ranking mechanism (for authors?)
	based on social interaction data collected through the wiki.
	They model the credibility of each contributor using the
	metric of centrality.
	
	They describe an ``acceptance factor'' which represents the
	percentage of the previous contributor's content that is
	visible after.  This is a similar idea to our own longevity,
        but they don't describe how they track a user's words except
        to say that they use the Wikipedia diff function.
	It's not clear how this was one.
    },
}

